    "prebuild": "npm run lint",
    "pretest": "npm run build",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "eslint": "^3.0.0",
    "eslint-plugin-vue-libs": "^1.2.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^3.2.2",
    "rxjs": "^5.2.0",
    "typescript": "^2.5.2",
    "vue": "^2.2.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue-libs/recommended"
